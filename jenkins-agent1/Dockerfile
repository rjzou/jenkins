FROM openjdk:17

ARG DEBIAN_FRONTEND=noninteractive
RUN apt-get update && apt-get install --assume-yes sshpass




sudo docker build -t myopenjdk:17 .



mvn jasypt:encrypt-value -Djasypt.encryptor.password="Hello@abc.com123" -Djasypt.plugin.value="root" -Djasypt.encryptor.algorithm="PBEWITHHMACSHA512ANDAES_256"
mvn jasypt:encrypt-value -Djasypt.encryptor.password="Hello@abc.com123" -Djasypt.plugin.value="123456" -Djasypt.encryptor.algorithm="PBEWITHHMACSHA512ANDAES_256"



mvn jasypt:encrypt-value -Djasypt.encryptor.password="Hello@abc.com123" -Djasypt.plugin.value="123456" -Djasypt.encryptor.algorithm="PBEWITHHMACSHA512ANDAES_256"


-----
加密：
mvn jasypt:encrypt-value -Djasypt.encryptor.password="Hello@abc.com123" -Djasypt.plugin.value="dbredis-ootkb0.serverless.apse1.cache.amazonaws.com" -Djasypt.encryptor.algorithm="PBEWITHHMACSHA512ANDAES_256"
解密：
mvn jasypt:decrypt-value -Djasypt.encryptor.password="Hello@abc.com123" -Djasypt.plugin.value="" -Djasypt.encryptor.algorithm="PBEWITHHMACSHA512ANDAES_256"



-----


